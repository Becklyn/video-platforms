{
    "name": "becklyn/video-platforms",
    "type": "symfony-bundle",
    "description": "Several helpers for integrating video tools in your Symfony application.",
    "homepage": "https://github.com/Becklyn/video-platforms",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "https://becklyn.com/"
        }
    ],
    "require": {
        "php": ">=7.4",
        "becklyn/rad-bundles": "^1.1.0",
        "symfony/form": "^v5.4.3 || ^v6.0.3",
        "symfony/http-foundation": "^v5.4.3 || ^v6.0.3",
        "symfony/http-kernel": "^v5.4.3 || ^v6.0.3",
        "symfony/validator": "^v5.4.3 || ^v6.0.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^v5.4.3 || ^v6.0.3"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-next": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\VideoPlatforms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\VideoPlatforms\\": "tests/"
        }
    }
}
